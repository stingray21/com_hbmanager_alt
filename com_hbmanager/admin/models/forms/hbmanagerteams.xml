			/>	 	 	 	
			<field 
				name="rankingName" 
				type="list" 
				default="" 
				label="in Tabelle" 
				description=""
				>
					<option value="leer">auswaehlen</option>
			</field>
			<field 
				name="scheduleName" 
				type="list" 
				default="" 
				label="in Spielplan" 
				description=""
				>
					<option value="leer">auswaehlen</option>
			</field>
			
			
